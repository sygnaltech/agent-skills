{
  "name": "webflow-designer-api",
  "description": "Webflow Designer API documentation and solutions.",
  "assets": [
    {
      "type": "copyFile",
      "src": "SKILL.md",
      "dest": "SKILL.md"
    },
    {
      "type": "copyFolder",
      "pattern": "solutions/**/*.md",
      "dest": "solutions"
    },
    {
      "type": "downloadFileList",
      "baseUrl": "https://developers.webflow.com",
      "listFile": "references.txt",
      "stripPrefix": "/designer",
      "dest": "references"
    }
  ]
}