{
  "name": "gitbook",
  "description": "A skill generated from a GitBook documentation source.",
  "assets": [
    {
      "type": "copyFile",
      "src": "SKILL.md",
      "dest": "SKILL.md"
    },
    {
      "type": "copyFolder",
      "pattern": "solutions/**/*.md",
      "dest": "solutions"
    },
    {
      "type": "downloadFileList",
      "baseUrl": "https://docs.example.com",
      "listFile": "references.txt",
      "stripPrefix": "",
      "dest": "references"
    }
  ]
}